{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "nevobo-beheer/nevobo-team-members",
	"version": "1.0.0",
	"title": "Teamleden",
	"category": "widgets",
	"icon": "list-view",
	"description": "Maak een lijst met teamleden.",
	"supports": {
		"html": false,
		"inserter": false,
		"multiple": false,
		"reusable": false,
		"lock": false
	},
	"textdomain": "nevobo-beheer",
	"attributes": {
		"teamMembers": {
			"type": "array",
			"array": {
				"number": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				},
				"role": {
					"type": "string"
				}
			},
			"default": [
				{
					"number": 1,
					"name": null,
					"role": "speler"
				}
			]
		}
	},
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}